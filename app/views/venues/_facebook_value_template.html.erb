<meta property="fb:app_id" content="317175341661486" />
<meta property="og:title" content="<%= t('.join_the_venue', :title => venue.title ) %>" /> 
<meta property="og:description" content="<%= venue.description %>" />  
<% if venue.host_kluuu.klu_images.empty? %>
	<meta property="og:image" content="" /> 
	<link rel="image_src" href="" />
<% else %>
<meta property="og:image" content="<%= 'http://'+ request.host + venue.host_kluuu.klu_images.first.image.url(:medium) %>" /> 
<link rel="image_src" href="<%= 'http://'+ request.host + venue.host_kluuu.klu_images.first.image.url(:medium) %>" />
<% end %>
