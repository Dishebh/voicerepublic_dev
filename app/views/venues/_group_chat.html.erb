<div class="group-chat-instance" id="group-chat-<%= venue.id %>">
	<div class="well well-large">
		<header class="label label-attention"><%= t('.venue_chat', :title => venue.title ) %></header>
		<ul class="chat-message unstyled scrollable-chat" id="<%= venue.chat_name %>">
			<!-- chat-messages to be here. renders chats/_single_chat_message.js.erb -->
		</ul>
	</div>
	<%= render(:partial => 'chats/group_form', :locals => { :venue => venue }) %>
</div>
<%= subscribe_to venue.channel_name %>