<div class="dashboard-contacts row-fluid">
	<div class="span6">
		<h3><%= t('.my_follower')%></h3>
		<ul class="unstyled">
			<%= render(:partial => '/follows/follower', :collection => @follower, :as => :follower_relation) %>
		</ul>
	</div>
	<div class="span6">
		<h3><%= t('.followed_by_me')%></h3>
		<ul class="unstyled">
			<%= render(:partial => '/follows/followed', :collection => @followed, :as => :followed_relation) %>
		</ul>
	</div>
</div>

