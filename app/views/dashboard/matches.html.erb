
<div class="tab-content dashboard-matches">
	<div class="row-fluid">
		<nav class="span4 dashboard-matches-own-klus">
			<h3 class=""><%= t('.your_klus')%></h3>
			<ul>
			<% @klus.each do |k| %>
					<li>
						<% if current_page?(dashboard_matches_path(:id => @matched_klu)) && k == @matched_klu  %>
							<%= link_to k.title, dashboard_matches_path(:id => k.id), :class => 'klu-title klu-layout active' %>
						<% else %>
							<%= link_to k.title, dashboard_matches_path(:id => k.id), :class => 'klu-title klu-layout' %>
						<% end %>
					</li>
			<% end %>
			</ul>
		</nav>
		<div class="span8">
			<h3><%= t('.matches_to_your_kluuu') %>:&nbsp; <span class="klu-title klu-layout"><%= @matched_klu.title if @matched_klu %></span></h3>
			<% @matching_klus.each do |k| %>
				<%= partial_for_klu(k,:medium) %>
			<% end unless @matching_klus.nil? %>
		</div>
	</div>
</div>