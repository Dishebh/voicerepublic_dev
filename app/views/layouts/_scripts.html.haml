#scripts
  - if user_signed_in?
    :javascript
      var pinMap = #{current_user.pin_map.to_json};

  = content_for :javascripts
  = javascript_include_tag :application

  - if Rails.env.production?
    = render 'shared/tawk'
    - unless @current_user.try(:insider?)
      = render 'shared/google_analytics'
