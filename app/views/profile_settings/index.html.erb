<%- model_class = ProfileSetting -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:timezone) %></th>
      <th><%= model_class.human_attribute_name(:language_1) %></th>
      <th><%= model_class.human_attribute_name(:language_2) %></th>
      <th><%= model_class.human_attribute_name(:language_3) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:about) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @profile_settings.each do |profile_setting| %>
      <tr>
        <td><%= link_to profile_setting.id, profile_setting_path(profile_setting) %></td>
        <td><%= profile_setting.timezone %></td>
        <td><%= profile_setting.language_1 %></td>
        <td><%= profile_setting.language_2 %></td>
        <td><%= profile_setting.language_3 %></td>
        <td><%= profile_setting.user_id %></td>
        <td><%= profile_setting.about %></td>
        <td><%=l profile_setting.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_profile_setting_path(profile_setting), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      profile_setting_path(profile_setting),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_profile_setting_path,
            :class => 'btn btn-primary' %>
            

