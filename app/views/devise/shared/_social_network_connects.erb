<%= link_to omniauth_authorize_path(resource_name, 'facebook'), :class => "btn signup-facebook" do %>
  <%= image_tag "connects/facebook_icon.png" %> 
  <%= t('.with', :default => "mit") %>&nbsp; facebook
<% end %>

<%= link_to omniauth_authorize_path(resource_name, 'google_oauth2'), :class => "btn signup-google" do %>
  <%= image_tag "connects/google_icon.png" %>
  <%= t('.with', :default => "mit") %>&nbsp; Google+
<% end %>