<% content_for :title do %>
	<%= @klu.title %>
<% end %>

<% content_for :facebook_values do %>
	<%= render(:partial => 'facebook_value_template', :locals => { :klu => @klu } ) %>
<% end %>

<% if @klu.instance_of?(NoKluuu) %>
	<%= render(:partial => 'no_kluuu', :locals => {:klu => @klu})%>
<% else %>
	<%= render(:partial => 'kluuu', :locals => {:klu => @klu})%>
<% end %>

